<h1>Listing diagnostics</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @diagnostics.each do |diagnostic| %>
  <tr>
    <td><%= diagnostic.name %></td>
    <td><%= diagnostic.description %></td>
    <td><%= link_to 'Show', diagnostic %></td>
    <td><%= link_to 'Edit', edit_diagnostic_path(diagnostic) %></td>
    <td><%= link_to 'Destroy', diagnostic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Diagnostic', new_diagnostic_path %>
